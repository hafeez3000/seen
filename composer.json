{
	"name": "thelfensdrfer/seen",
	"description": "SEEN",
	"keywords": ["seen", "tv", "movie", "database"],
	"homepage": "https://seenapp.com/",
	"type": "project",
	"license": "CC-BY-NC-4.0",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"php": ">=5.4.0",
		"yiisoft/yii2": "2.0.*",
		"yiisoft/yii2-bootstrap": "2.0.*",
		"yiisoft/yii2-jui": "2.0.*",
		"yiisoft/yii2-redis": "2.0.*",
		"zelenin/yii2-slug-behavior": "0.2.*",
		"kartik-v/yii2-grid": "2.8.*",
		"kartik-v/yii2-mpdf": "1.0.*",
		"nesbot/Carbon": "1.13.*",
		"mailchimp/mailchimp": "2.0.*",
		"mandrill/mandrill": "1.0.*",
		"bugsnag/bugsnag": "2.*",
		"gabrielbull/browser": "3.1.*",
		"shark/simple_html_dom": "dev-master",
		"lusitanian/oauth": "0.3.*",
		"predis/predis": "1.0.*",
		"predictionio/predictionio": "0.7.*",
		"bower-asset/bootstrap": "3.3.*",
		"bower-asset/bootswatch": "3.3.*",
		"bower-asset/select2": "3.5.*",
		"bower-asset/select2-bootstrap-css": "1.4.*",
		"bower-asset/moment": "2.6.*",
		"bower-asset/holderjs": "2.3.*",
		"bower-asset/jquery": "2.1.*",
		"bower-asset/highcharts-release": "4.0.*",
		"bower-asset/jquery-cookie": "1.4.*"
	},
	"require-dev": {
		"yiisoft/yii2-debug": "*",
		"yiisoft/yii2-gii": "*",
		"yiisoft/yii2-codeception": "*",
		"codeception/codeception": "1.8.*",
		"codeception/specify": "0.3.*",
		"codeception/verify": "0.2.*",
		"thelfensdrfer/yii2-ssh-console": "1.0.*"

	},
	"scripts": {
		"post-create-project-cmd": [
			"yii\\composer\\Installer::setPermission"
		],
		"post-install-cmd": [
			"npm install"
		],
		"post-update-cmd": [
			"npm update"
		]
	},
	"config": {
		"process-timeout": 1800,
		"store-auths": true
	},
	"extra": {
		"yii\\composer\\Installer::postCreateProject": {
			"setPermission": [
				{
					"runtime": "0777",
					"web/assets": "0777",
					"yii": "0755"
				}
			],
			"generateCookieValidationKey": [
				"config/web.php"
			]
		},
		"asset-installer-paths": {
			"bower-asset-library": "vendor/bower"
		}
	}
}
